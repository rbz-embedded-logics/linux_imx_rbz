/dts-v1/;

#include <dt-bindings/phy/phy-imx8-pcie.h>
#include "../freescale/imx8mp.dtsi"
#include "mod-imx8m-plus.dtsi"

/ {
	model = "RBZ Carrier Basic MOD_IMX8M_PLUS";
	compatible = "fsl,imx8mp-evk", "fsl,imx8mp";
};

&ecspi1 {
  status = "okay";

  at25@0 {
    compatible = "atmel,at25";
    reg = <0>;
    spi-max-frequency = <500000>;
    pagesize = <8>;
    size = <128>;
    address-width = <8>;
  };
  
  at25@1 {
    compatible = "atmel,at25";
    reg = <1>;
    spi-max-frequency = <500000>;
    pagesize = <8>;
    size = <128>;
    address-width = <8>;
  };
};

&ecspi3 {
  status = "okay";

  at25@0 {
    compatible = "atmel,at25";
    reg = <0>;
    spi-max-frequency = <500000>;
    pagesize = <8>;
    size = <128>;
    address-width = <8>;
  };
  
  at25@1 {
    compatible = "atmel,at25";
    reg = <1>;
    spi-max-frequency = <500000>;
    pagesize = <8>;
    size = <128>;
    address-width = <8>;
  };
};
